<Window x:Name="resultWindow" x:Class="_5Kurs_Sybil.ResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        Title="ResultWindow" Height="400" Width="500" Loaded="resultWindow_Loaded">
    <Grid>
        <d3:ChartPlotter x:Name="plotter" Margin="175,10,5,5"/>
        <ScrollViewer HorizontalAlignment="Left" Margin="10,10,0,5" Width="160">
            <DataGrid x:Name="dataGrid" CanUserAddRows="False" ColumnWidth="*"/>
        </ScrollViewer>
    </Grid>
</Window>
